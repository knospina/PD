{% extends 'EKPDBundle::base.html.twig' %}

{% block content %}

<header id="main_nav">
    <nav>
        <ul>
            <li>{{ include(
            'EKPDBundle:User:navigation.html.twig'
            ) }}</li>
        </ul>
    </nav>
</header>

<main>
    <section>

        <article>
            <header>
                <h2>Te būs user jaunumi</h2>
                <p>Wish lists, dzimšanas diena</p>
            </header>

            <ul>
                <li>{{ wish.id }}, {{ wish.name }}, {{ wish.url }}</li>
            </ul>
            <p>Fulfill</p>
            {{ form(fulfill_form) }}

        </article>

    </section>
    {{ include(
            'EKPDBundle:User:profile.html.twig'
            ) }}

</main>

<footer>
    <p>Wish list 2013 Evita Knospiņa</p>
</footer>


{% endblock %}