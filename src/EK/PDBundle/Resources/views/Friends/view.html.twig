{% extends 'EKPDBundle::base.html.twig' %}

{% block content %}

<header id="main_nav">
    <nav>
        <ul>
            <li>{{ include(
            'EKPDBundle:User:navigation.html.twig'
            ) }}</li>
        </ul>
    </nav>
</header>

<main>
    <section>

        <article>
            <header>
                <h2>Te būs feed title</h2>
                <p>Feeda laiks, autors utt</p>
            </header>
                {{ userId }}

            <ul>
                {% for key, friend in friends %}
                <li>{{ friend.id }}, {{ friend.first_name }}, {{ friend.last_name }}</li>
                {% endfor %}
            </ul>
        </article>

    </section>
    {{ include(
            'EKPDBundle:User:profile.html.twig'
            ) }}

</main>

<footer>
    <p>Wish list 2013 Evita Knospiņa</p>
</footer>


{% endblock %}